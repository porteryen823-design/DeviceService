# API 基礎 URL
VITE_API_BASE_URL=http://localhost:5200

# 應用程式設定
VITE_APP_TITLE=Device Service Management
VITE_APP_VERSION=1.0.0

# 開發環境設定
VITE_ENABLE_MSW=false
VITE_ENABLE_REDUX_DEVTOOLS=true

# MQTT 設定
VITE_MQTT_ENABLED=true
VITE_MQTT_HOST=localhost
VITE_MQTT_PORT=9001
VITE_MQTT_PROTOCOL=ws
VITE_MQTT_PATH=/mqtt
VITE_MQTT_USERNAME=
VITE_MQTT_PASSWORD=
VITE_MQTT_CLIENT_ID=frontend_mqtt_client
VITE_MQTT_RECONNECT_ENABLED=true
VITE_MQTT_RECONNECT_MAX_ATTEMPTS=5
VITE_MQTT_RECONNECT_DELAY=3000
VITE_MQTT_LOGGING_ENABLED=true
VITE_MQTT_LOGGING_MAX_ENTRIES=100
VITE_MQTT_CACHE_TIMEOUT=300000
VITE_MQTT_CACHE_MAX_SIZE=1000